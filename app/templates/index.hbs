<h1>Bustle Posts</h1>

{{#each model as |post|}}
  By {{post.owner}}  <br>{{post.head}}, <br>

  <h2>{{post.snippet}}</h2> <br>
  <h4>{{post.body}}</h4> <br>

  <p>
    <img src={{post.image}} alt=":(" />
  </p>
{{post-tile post=post update="update" destroyPost="destroyPost"}}
{{/each}}
{{new-post savePost="savePost"}}
